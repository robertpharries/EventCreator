{#
/**
 * @file anonyreg.html.twig
 * Default theme implementation to present Anonyreg data.
 *
 * This template is used when viewing Anonyreg pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_anonyreg()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('anonyreg') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
